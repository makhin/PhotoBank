{
  "name": "@photobank/shared",
  "version": "1.0.1",
  "type": "module",
  "description": "Shared utilities and types for Photobank",
  "private": true,
  "engines": {
    "node": ">=20.11"
  },
  "exports": {
    "./package.json": "./package.json",
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "default": "./dist/auth/index.js"
    },
    "./safeStorage": "./src/safeStorage.ts",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./index": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./format": {
      "types": "./dist/format/index.d.ts",
      "default": "./dist/format/index.js"
    },
    "./metadata": {
      "types": "./dist/metadata/index.d.ts",
      "default": "./dist/metadata/index.js"
    },
    "./ai/openai": {
      "types": "./dist/ai/openai.d.ts",
      "default": "./dist/ai/openai.js"
    },
    "./types/problem": {
      "types": "./dist/types/problem.d.ts",
      "default": "./dist/types/problem.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "default": "./dist/constants.js"
    },
    "./utils/logger": {
      "types": "./dist/utils/logger.d.ts",
      "default": "./dist/utils/logger.js"
    },
    "./api/photobank": {
      "types": "./dist/api/photobank/index.d.ts",
      "default": "./dist/api/photobank/index.js"
    },
    "./api/photobank/*": {
      "types": "./dist/api/photobank/*.d.ts",
      "default": "./dist/api/photobank/*.js"
    }
  },
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -b --force",
    "build:types": "pnpm run build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "fuse.js": "^7.1.0",
    "lru-cache": "^11.2.2",
    "object-hash": "^3.0.0",
    "openai": "^6.6.0",
    "p-queue": "^9.0.0",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@tanstack/query-core": "^5.87.4",
    "@tanstack/react-query": "^5.87.4",
    "react": "^19.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@tanstack/query-core": "^5.90.5",
    "@tanstack/react-query": "^5.90.5",
    "@types/react": "^19.2.2",
    "msw": "^2.11.6",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.1"
  }
}
