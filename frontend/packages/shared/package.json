{
  "name": "@photobank/shared",
  "version": "1.0.1",
  "type": "module",
  "description": "Shared utilities and types for Photobank",
  "private": true,
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./auth": "./src/auth.ts",
    "./safeStorage": "./src/safeStorage.ts",
    "./constants": "./src/constants.ts",
    "./format": "./src/format.ts",
    "./utils/logger": "./src/utils/logger.ts",
    "./ai/openai": "./src/ai/openai.ts",
    "./api/photobank": "./src/api/photobank/index.ts",
    "./api/photobank/*": "./src/api/photobank/*"
  },
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:types": "rimraf dist && tsc -p tsconfig.json --emitDeclarationOnly",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.0",
    "eventemitter3": "^5.0.1",
    "lru-cache": "^11.2.1",
    "object-hash": "^3.0.0",
    "openai": "^5.20.1",
    "p-queue": "^8.1.1",
    "p-timeout": "^6.1.4",
    "zod": "^4.1.7"
  },
  "peerDependencies": {
    "@tanstack/query-core": "^5.87.4",
    "@tanstack/react-query": "^5.87.4",
    "react": "^19.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@tanstack/query-core": "^5.87.4",
    "@tanstack/react-query": "^5.87.4",
    "@types/react": "^19.1.12",
    "msw": "^2.11.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
