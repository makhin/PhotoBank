{
  "name": "@photobank/telegram-bot",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && cpy \"src/locales/**/*\" dist/locales",
    "prestart": "pnpm run build",
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@grammyjs/conversations": "^2.1.0",
    "@grammyjs/i18n": "^1.1.2",
    "@grammyjs/runner": "^2.0.3",
    "@photobank/shared": "workspace:*",
    "axios": "^1.11.0",
    "bottleneck": "^2.19.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "fetch-blob": "^4.0.0",
    "grammy": "^1.38.2",
    "lru-cache": "^11.2.1",
    "p-retry": "^7.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "cpy-cli": "^6.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
