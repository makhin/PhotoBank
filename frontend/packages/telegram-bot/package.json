{
  "name": "@photobank/telegram-bot",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.11"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "pretypecheck": "pnpm --filter @photobank/shared run build:types",
    "typecheck": "tsc -b",
    "build": "pnpm run typecheck && tsup && pnpm run copy:locales",
    "copy:locales": "cpy src/locales/**/*.ftl dist/locales",
    "dev": "tsup --watch --onSuccess \"pnpm run copy:locales && node dist/index.js\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@grammyjs/conversations": "^2.1.0",
    "@grammyjs/i18n": "^1.1.2",
    "@grammyjs/runner": "^2.0.3",
    "@photobank/shared": "workspace:*",
    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.12.2",
    "bottleneck": "^2.19.5",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "dotenv": "^17.2.3",
    "fuse.js": "^7.1.0",
    "grammy": "^1.38.3",
    "lru-cache": "^11.2.2",
    "object-hash": "^3.0.0",
    "openai": "^6.6.0",
    "p-queue": "^9.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "cpy-cli": "^6.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.1"
  }
}
