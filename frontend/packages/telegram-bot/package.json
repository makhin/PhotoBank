{
  "name": "@photobank/telegram-bot",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": { "node": ">=20.11" },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "pretypecheck": "pnpm --filter @photobank/shared run build:types",
    "typecheck": "tsc -b",
    "build": "pnpm run typecheck && tsup",
    "dev": "tsup --watch --onSuccess \"node dist/index.js\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@photobank/shared": "workspace:*",
    "@grammyjs/conversations": "^2.1.0",
    "@grammyjs/i18n": "^1.1.2",
    "@grammyjs/runner": "^2.0.3",
    "axios": "^1.11.0",
    "bottleneck": "^2.19.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "fetch-blob": "^4.0.0",
    "grammy": "^1.38.2",
    "lru-cache": "^11.2.1",
    "object-hash": "^3.0.0",
    "openai": "^5.20.1",
    "p-retry": "^7.0.0",
    "zod": "^4.1.7"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "cpy-cli": "^6.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
