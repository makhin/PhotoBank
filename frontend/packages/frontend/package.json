    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-redux": "^0.1.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "msw": "^2.11.1",
    "prettier": "^3.6.2",
    "tailwindcss-debug-screens": "^3.0.1",
    "tw-animate-css": "^1.3.8",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  }
