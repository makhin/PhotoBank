{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "noEmit": true,
    "verbatimModuleSyntax": true,
    "resolvePackageJsonExports": true,
    "resolvePackageJsonImports": true,
    "baseUrl": ".",
    "paths": {
      "@photobank/shared": ["packages/shared/src"],
      "@photobank/shared/*": ["packages/shared/src/*"]
    }
  }
}
